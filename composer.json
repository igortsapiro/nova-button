{
    "name": "igortsapiro/nova-button",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Barracode\\Button\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0"
        },
        "laravel": {
            "providers": [
                "Barracode\\Button\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
